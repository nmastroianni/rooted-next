{
  "format": "page",
  "id": "clinician",
  "label": "Clinician",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "full_name": {
        "type": "StructuredText",
        "config": {
          "label": "Full Name",
          "placeholder": "Enter the clinician's full name",
          "allowTargetBlank": true,
          "single": "heading1"
        }
      },
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "first_name": {
        "type": "Text",
        "config": {
          "label": "First Name",
          "placeholder": ""
        }
      },
      "portrait": {
        "type": "Image",
        "config": {
          "label": "Portrait",
          "constraint": {},
          "thumbnails": []
        }
      },
      "focuses": {
        "type": "Group",
        "config": {
          "label": "Focuses",
          "repeat": true,
          "fields": {
            "focus": {
              "type": "Text",
              "config": {
                "label": "Focus",
                "placeholder": "Enter a focus"
              }
            }
          }
        }
      },
      "approaches": {
        "type": "Group",
        "config": {
          "label": "Approaches",
          "repeat": true,
          "fields": {
            "approach": {
              "type": "Text",
              "config": {
                "label": "Approach",
                "placeholder": ""
              }
            }
          }
        }
      },
      "about": {
        "type": "StructuredText",
        "config": {
          "label": "About",
          "placeholder": "Enter a description of the clinician",
          "allowTargetBlank": true,
          "multi": "paragraph,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "services": {
        "type": "Group",
        "config": {
          "label": "Services",
          "repeat": true,
          "fields": {
            "service": {
              "type": "Link",
              "config": {
                "label": "Service",
                "select": "document",
                "customtypes": ["service"]
              }
            }
          }
        }
      },
      "specialties": {
        "type": "StructuredText",
        "config": {
          "label": "Specialties",
          "placeholder": "Make a list of specialties",
          "allowTargetBlank": true,
          "multi": "list-item"
        }
      },
      "psychtoday": {
        "type": "Link",
        "config": {
          "label": "Psychtoday",
          "placeholder": "",
          "select": null
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "rich_text": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      },
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      }
    }
  }
}
